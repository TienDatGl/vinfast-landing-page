    "@nuxtjs/seo": "^2.0.0-rc.10",
    "@nuxtjs/sitemap": "7.2.6",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@nuxtjs/vercel-builder": "^0.25.0",
    "@storefront-ui/vue": "^2.6.3",
    "@types/nodemailer": "^6.4.17",
    "@vueuse/core": "^13.2.0",
    "@vueuse/nuxt": "^10.9.0",
    "firebase": "^11.10.0",
