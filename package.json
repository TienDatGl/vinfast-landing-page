{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --log-level verbose",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build:functions": "rm -rf functions/lib && rm -rf functions/.output && cp -r .output functions/.output && cd functions && npm install && npm run build && cd ..",
