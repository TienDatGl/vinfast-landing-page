  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.17.0",
    "@storefront-ui/typography": "^2.6.1",
    "autoprefixer": "^10.4.15",
